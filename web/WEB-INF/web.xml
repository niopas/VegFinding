<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee           http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>UthLogin</servlet-name>
        <servlet-class>com.example.web.UthLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DbRegister</servlet-name>
        <servlet-class>com.example.web.DbRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DbLogin</servlet-name>
        <servlet-class>com.example.web.DbLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DbRegister</servlet-name>
        <url-pattern>/DbRegister.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UthLogin</servlet-name>
        <url-pattern>/UthLogin.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>DbLogin</servlet-name>
        <url-pattern>/DbLogin.do</url-pattern>
    </servlet-mapping>
</web-app>